		"@types/bn.js": "^4.11.6",
		"@types/chai": "^4.2.11",
		"@types/mocha": "^7.0.2",
		"@types/web3": "^1.2.2",
		"truffle": "^5.1.22",
		"typechain": "2.0.0",
		"typescript": "4.1.5",
		"web3": "^1.3.0"
	}
